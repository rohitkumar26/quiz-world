<% if(typeof finalexamresult!== 'undefined'){ %>
<div class="columns is-centered">
  <div class="column is-half has-text-centered">
    <h1 class="title is-4 is-capitalized">your correct answers</h1>
    <h1 class="title is-5"><%= correctanswer %></h1>
    <h1 class="title is-4 is-capitalized">your wrong answers</h1>
    <h1 class="title is-5"><%= finalexamresult.length %></h1>
  </div>
</div>
<% if(finalexamresult.length===0) {%>
<h1 class="title is-4 is-capitalized has-text-centered has-text-link">
  congratulation ! you answered all questions correctly
</h1>
<% } %> <% } %> <% if (wronganswer.length>0){ %>
<h1 class="title is-3 is-capitalized">correct answers</h1>
<% wronganswer.forEach(element => { %>
<h1 class="title is-4">Q) &nbsp; <%= element.question %></h1>
<p
  class="title is-5 py-5 is-capitalized has-text-centered has-text-dark has-background-white"
>
  Ans: &nbsp;<%= element.correctanswer %>
</p>
<hr />

<% }); %> <% } %>
<p class="has-text-centered">
  <a href="/" class="button is-info">Back To Home page</a>
</p>
